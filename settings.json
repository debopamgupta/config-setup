      },
      {
        "acrylicOpacity": 0.69999999999999996,
        "closeOnExit": "graceful",
        "commandline": "C:\\Program Files\\Git\\bin\\bash.exe --cd-to-home",
        "cursorColor": "#ECEFF4",
        "cursorShape": "underscore",
