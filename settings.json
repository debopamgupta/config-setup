        "guid": "{72a69aca-322e-465d-a799-1d64dff65269}",
        "historySize": 9001,
        "icon": "C:/Program Files/Git/mingw64/share/git/git-for-windows.ico",
        "name": "Git Bash",
        "padding": "5, 0, 0, 0",
        "snapOnInput": true,
        "startingDirectory": "%USERPROFILE%",
        "suppressApplicationTitle": true,
        "tabTitle": "Bash",
        "useAcrylic": false
      }
